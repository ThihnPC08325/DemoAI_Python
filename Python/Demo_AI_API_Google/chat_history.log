```C#
// Đính kèm script này vào đối tượng nhân vật
using UnityEngine;

public class ThirdPersonMovement : MonoBehaviour
{
    public CharacterController controller; // Bộ điều khiển nhân vật
    public float speed = 5.0f; // Tốc độ di chuyển của nhân vật
    public float jumpForce = 5.0f; // Lực nhảy của nhân vật
    public float gravity = 9.81f; // Trọng lực

    private Vector3 velocity; // Vận tốc của nhân vật

    private void Update()
    {
        // Nhận đầu vào từ người dùng
        float horizontalInput = Input.GetAxis("Horizontal");
        float verticalInput = Input.GetAxis("Vertical");

        // Tính toán hướng di chuyển
        Vector3 moveDirection = transform.right * horizontalInput + transform.forward * verticalInput;

        // Di chuyển nhân vật
        controller.Move(moveDirection * speed * Time.deltaTime);

        // Nhảy
        if (Input.GetKeyDown(KeyCode.Space))
        {
            velocity.y = jumpForce;
        }

        // Áp dụng trọng lực
        velocity.y -= gravity * Time.deltaTime;

        // Di chuyển nhân vật theo chiều dọc
        controller.Move(velocity * Time.deltaTime);
    }
}
```

